<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:resources="clr-namespace:Boxy_Core.Views.Resources">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary
            Source="pack://application:,,,/Views/Resources/Views.xaml" />
        <ResourceDictionary
            Source="pack://application:,,,/Views/Style/Colors.xaml" />
        <ResourceDictionary
            Source="pack://application:,,,/Views/Style/VectorData.xaml" />
        <ResourceDictionary
            Source="pack://application:,,,/Views/Style/ControlStyles.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <resources:ViewModelLocator
        x:Key="ViewModelLocator" />

</ResourceDictionary>